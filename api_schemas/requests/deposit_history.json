{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "request/deposit_history",
  "title": "Body of the deposit_history API method sent from the client.",
  "type": "object",
  "properties": {
    "v": {
      "type": "string"
    },
    "api_key": {
      "type": "string"
    },
    "from": {
      "type": "string",
      "format": "date-time"
    },
    "to": {
      "type": "string",
      "format": "date-time"
    },
    "limit": {
      "type": "integer",
      "exclusiveMinimum": 0,
      "maximum": 10
    },
    "offset": {
      "type": "integer",
      "minimum": 0,
      "maximum": 1000000
    }
  },
  "required": [
    "v",
    "api_key"
  ]
}
