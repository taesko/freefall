{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "request/admin_list_account_transfers",
  "title": "Admin list account transfers request",
  "type": "object",
  "properties": {
    "v": {
      "type": "string",
      "title": "API version"
    },
    "api_key": {
      "type": "string",
      "title": "API key"
    },
    "filters": {
      "title": "Filters",
      "type": "object",
      "properties": {
        "user_email": {
          "title": "User email filter",
          "type": "string"
        },
        "datetime_from": {
          "title": "Datetime from fiter",
          "type": "string"
        },
        "datetime_to": {
          "title": "Datetime to filter",
          "type": "string"
        },
        "type": {
          "title": "Account transfer type filter",
          "type": "string",
          "enum": [
            "all",
            "deposits",
            "withdrawals"
          ]
        },
        "reason": {
          "title": "Account transfer reason filter",
          "type": "string",
          "enum": [
            "all",
            "employee",
            "new-subscription",
            "fetch"
          ]
        }
      },
      "required": [
        "type",
        "reason"
      ]
    },
    "groups": {
      "title": "Groups",
      "type": "object",
      "properties": {
        "user_email": {
          "title": "User email grouping",
          "type": "string",
          "enum": [
            "none",
            "email"
          ]
        },
        "datetime": {
          "title": "Datetime grouping",
          "type": "string",
          "enum": [
            "second",
            "minute",
            "hour",
            "day",
            "week",
            "month",
            "year"
          ]
        }
      },
      "required": [
        "user_email",
        "datetime"
      ]
    }
  },
  "required": [
    "v",
    "api_key",
    "filters",
    "groups"
  ]
}
