{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "response/admin_list_users",
  "title": "Admin list users response",
  "type": "object",
  "properties": {
    "users": {
      "type": "array",
      "title": "List of users",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "title": "User id"
          },
          "email": {
            "type": "string",
            "title": "User email"
          },
          "credits": {
            "type": "number",
            "title": "User credits",
            "minimum": 0
          },
          "api_key": {
            "title": "API key",
            "type": "string"
          },
          "verified": {
            "title": "Verified",
            "type": "boolean"
          },
          "verification_token": {
            "title": "Verification token",
            "type": "string"
          },
          "active": {
            "title": "Active",
            "type": "boolean"
          }
        },
        "required": [
          "id",
          "email",
          "credits",
          "api_key",
          "verified",
          "verification_token",
          "active"
        ]
      }
    }
  },
  "required": ["users"]
}
