{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "response/list_subscriptions",
  "title": "Body of the response of the list_subscriptions API method sent TO the client.",
  "type": "object",
  "properties": {
    "subscriptions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "fly_from": {
            "type": "string"
          },
          "fly_to": {
            "type": "string"
          },
          "date_from": {
            "type": "string",
            "format": "date"
          },
          "date_to": {
            "type": "string",
            "format": "date"
          },
          "plan": {
            "type": "string",
            "enum": ["daily", "weekly", "monthly"]
          }
        },
        "required": [
          "id",
          "fly_from",
          "fly_to",
          "date_from",
          "date_to"
        ]
      }
    }
  },
  "required": [
    "subscriptions"
  ]
}